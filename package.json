{
	"name": "dd-api",
	"version": "1.7.35-2",
	"description": "API for DropDesk",
	"readme": "./README.ms",
	"private": true,
	"scripts": {
		"generate-keys": "node ./generate-keys.js",
		"lint": "eslint ./src --ext .ts",
		"lint:fix": "eslint ./src --ext .ts --fix",
		"debug": "nodemon -e ts -x \"node --inspect=0.0.0.0:9229 -r ts-node/register -r tsconfig-paths/register ./src/index.ts\"",
		"start": "env-cmd -f ./.env ts-node -r reflect-metadata -r tsconfig-paths/register ./src/index.ts",
		"docs:api": "jsdoc2md --configure ./jsdoc.json --files src/**/*.ts > docs/api.md",
		"docs:build": "npm run docs:prepare && npm run docs:api && gitbook build",
		"docs:serve": "npm run docs:prepare && npm run docs:api && gitbook serve",
		"docs:clean": "rimraf _book",
		"docs:prepare": "gitbook install",
		"version": "",
		"postversion": "git push && git push --tags",
		"version:release": "yarn version --prepatch -m \"Release %s\"",
		"version:hotfix": "yarn version --prerelease -m \"Pre-release %s\"",
		"test": "env-cmd jest",
		"test:coverage": "yarn test --coverage --collectCoverageFrom=\"./src/**\"",
		"test:unit": "env-cmd -f ./.env jest --config=./jest.unit.config.ts --testPathIgnorePatterns=src/controller/* --testPathIgnorePatterns=src/utils/tests/* --ci --forceExit",
		"test:int": "env-cmd -f ./.env.test jest --config=./jest.int.config.ts --testPathPattern=.*.controller.spec.ts --ci --forceExit",
		"typeorm": "env-cmd -f ./.env.test ts-node -r reflect-metadata -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/main-data-source.ts",
		"migration:run": "yarn run schema:drop && yarn run schema:sync && yarn run typeorm migration:run",
		"migration:gen": "yarn run typeorm migration:generate -- -n Amenities -d ./src/migrations/",
		"schema:drop": "yarn run typeorm schema:drop",
		"schema:sync": "yarn run typeorm schema:sync"
	},
